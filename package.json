{
  "name": "@einsatzplan/source",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "start": "nx run einsatzplan:serve",
    "test": "nx run-many --target=test --parallel=4",
    "lint": "nx run-many --target=lint --parallel=4",
    "lint:fix": "nx run-many --target=lint --parallel=4 --fix",
    "build:einsatzplan": "nx run einsatzplan:build:production",
    "prepush": "nx run-many --targets=test,lint,build --configuration=production --nxBail --parallel=4",
    "scrape-all": "nx run data-scraper:serve:all",
    "scrape-championships": "nx run data-scraper:serve:championships",
    "scrape-matches": "nx run data-scraper:serve:matches",
    "scrape-players": "nx run data-scraper:serve:players",
    "scrape-teams": "nx run data-scraper:serve:teams",
    "analyze:einsatzplan": "run-s --continue-on-error build:einsatzplan analyze:einsatzplan:visualize",
    "analyze:einsatzplan:visualize": "esbuild-visualizer --metadata dist/apps/einsatzplan/stats.json --filename dist/apps/einsatzplan/stats.html"
  },
  "private": true,
