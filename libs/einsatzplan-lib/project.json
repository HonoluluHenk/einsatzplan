{
  "name": "einsatzplan-lib",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/einsatzplan-lib/src",
  "prefix": "epla",
  "projectType": "library",
  "tags": [
    "type:app scope:einsatzplaan"
  ],
  "targets": {
    "build": {
      "executor": "@nx/angular:ng-packagr-lite",
      "outputs": [
        "{workspaceRoot}/dist/{projectRoot}"
      ],
      "options": {
        "project": "libs/einsatzplan-lib/ng-package.json"
      },
      "configurations": {
        "production": {
          "tsConfig": "libs/einsatzplan-lib/tsconfig.lib.prod.json"
        },
        "development": {
          "tsConfig": "libs/einsatzplan-lib/tsconfig.lib.json"
        }
      },
      "defaultConfiguration": "production"
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": [
        "{workspaceRoot}/coverage/{projectRoot}"
      ],
      "options": {
        "jestConfig": "libs/einsatzplan-lib/jest.config.ts"
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint"
    }
  }
}
